{
    "name": "downtempo-stack",
    "version": "1.0.0",
    "main": "index.js",
    "license": "MIT",
    "scripts": {
        "build": "run-s build:*",
        "build:css": "npm run generate:css -- --minify",
        "build:remix": "remix build",
        "dev": "run-p dev:*",
        "dev:remix": "cross-env NODE_ENV=development binode --require ./mocks -- @remix-run/dev:remix dev",
        "lint": "eslint --cache --cache-location ./node_modules/.cache/eslint .",
        "format": "prettier --write ."
    },
    "prettier": {
        "trailingComma": "es5",
        "tabWidth": 4,
        "semi": false,
        "singleQuote": true,
        "bracketSpacing": false,
        "printWidth": 80
    },
    "eslintIgnore": [
        "/node_modules",
        "/build",
        "/public/build"
    ],
    "dependencies": {
        "@remix-run/node": "^1.11.0",
        "@remix-run/react": "^1.11.0",
        "@remix-run/serve": "^1.11.0",
        "@remix-run/server-runtime": "^1.11.0",
        "isbot": "^3.6.5",
        "msw": "^0.49.3",
        "react": "^18.2.0",
        "react-dom": "^18.2.0"
    },
    "devDependencies": {
        "@remix-run/dev": "^1.11.0",
        "@remix-run/eslint-config": "^1.11.1",
        "@types/eslint": "^8.4.10",
        "@types/node": "^18.11.18",
        "@types/react": "^18.0.27",
        "@types/react-dom": "^18.0.10",
        "binode": "^1.0.5",
        "cross-env": "^7.0.3",
        "eslint": "^8.32.0",
        "eslint-config-prettier": "^8.6.0",
        "npm-run-all": "^4.1.5",
        "prettier": "^2.8.3",
        "ts-node": "^10.9.1",
        "typescript": "^4.9.4",
        "vite": "^4.0.4",
        "vite-tsconfig-paths": "^4.0.5"
    },
    "engines": {
        "node": ">=14"
    }
}
